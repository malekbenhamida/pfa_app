<h1>Listing users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Last name</th>
    <th>Age</th>
    <th>Gender</th>
    <th>Profession</th>
    <th>Ethnicity</th>
    <th>Marital status</th>
    <th>Annuel income</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.age %></td>
    <td><%= user.gender %></td>
    <td><%= user.profession %></td>
    <td><%= user.ethnicity %></td>
    <td><%= user.marital_status %></td>
    <td><%= user.annuel_income %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

